<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nagel.phonebook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.tan.MainActivity"
            android:label="@string/main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.tan.SearchActivity"
            android:label="@string/peopleSearch"/>
        <activity android:name="com.example.tan.PersonActivity"
            android:label="@string/person"/>
        <activity android:name="com.example.tan.PeopleActivity"
            android:label="@string/peopleList"/>
    </application>

</manifest>